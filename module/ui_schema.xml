          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
      </group>
      <group ref="Historical_Info">
        <label>{Historical_Info}</label>
        <input faims_attribute_type="measure" ref="Lease_Number" faims_attribute_name="Lease Number">
