  <h:head>
    <h:title>Fill This In</h:title>
    <model>
      <instance>
        <faims id="Fill_This_In">
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <Login>
            <Login>
              <User/>
              <Log_in/>
              <Sign_up/>
            </Login>
          </Login>
          <Control>
            <Control>
              <New_Cemetery/>
              <List_of_Existing_Cemeteries/>
            </Control>
            <Plots>
              <Plots/>
            </Plots>
            <Search>
              <Colgroup_0>
                <Col_0>
                  <Search_Term/>
                </Col_0>
                <Col_1>
                  <Search_Button/>
                </Col_1>
              </Colgroup_0>
