        </input>
        <select type="camera" faims_attribute_type="measure" faims_sync="true" ref="Photos" faims_attribute_name="Photos">
          <label>{Photos}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <trigger ref="Button_Photos">
          <label>{Button_Photos}</label>
        </trigger>
      </group>
      <group ref="Plot">
        <label>{Plot}</label>
        <select1 faims_attribute_type="vocab" ref="Plot_Type" faims_attribute_name="Plot Type">
